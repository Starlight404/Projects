library("RMySQL")


con<- dbConnect(MySQL(),
                username = "root",
                dbname = "shoestore")

dbListTables(con)


dbListFields(con, "employee")

data1<- dbGetQuery(con,"select * from employee")
View(data1)


rs<-   dbSendQuery(con, "select * from form")
rs

data<- fetch(rs, n = 2)# HERE N shows the number of rows, -1 means all
View(data)

